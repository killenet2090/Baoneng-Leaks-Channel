<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bnmotor.icv.tsp.device.mapper.VehicleBrandMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bnmotor.icv.tsp.device.model.entity.VehicleBrandPo">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="del_flag" property="delFlag" />
        <result column="version" property="version" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id, code, name, del_flag, version, create_by, create_time, update_by, update_time
    </sql>

    <select id="selectAll" resultType="VehicleBrandPo">
        select <include refid="Base_Column_List" />
        from tb_vehicle_brand
        where del_flag = 0
    </select>
</mapper>
